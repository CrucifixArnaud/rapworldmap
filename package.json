{
  "name": "rapworldmap",
  "version": "1.0.0",
  "description": "Display a map of Rapper based on their childhood neighborhood",
  "main": "rapworldmap.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev:css": "brunch watch",
    "dev:js": "watchify -t [ babelify ] public/js/atlas.js -o public/js/atlas.bundle.js",
    "build:css": "node-sass --include-path scss public/scss/main.scss public/css/main.css | postcss -u autoprefixer -r public/css/main.css | postcss -u cssnano -r public/css/main.css",
    "build:js": "browserify public/js/atlas.js -t [ babelify --presets [ es2015 react ] ] | uglifyjs -mc -o public/js/atlas.bundle.js",
    "build": "NODE_ENV=production npm run build:js && npm run build:css",
    "start:dev": "nodemon rapworldmap.js",
    "start": "NODE_ENV=production node rapworldmap.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/CrucifixArnaud/rapmap.git"
  },
  "author": "Crucifix Arnaud",
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/CrucifixArnaud/rapmap/issues"
  },
  "homepage": "https://gitlab.com/CrucifixArnaud/rapmap#README",
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.15.2",
    "client-sessions": "^0.7.0",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.3",
    "dotenv": "^2.0.0",
    "ejs": "^2.5.2",
    "express": "^4.14.0",
    "express-ejs-layouts": "^2.2.0",
    "express-validator": "^2.21.0",
    "leaflet.markercluster": "1.0.0",
    "mapbox.js": "3.0.1",
    "moment": "^2.15.2",
    "mongoose": "^4.6.4",
    "morgan": "^1.7.0",
    "multer": "^1.2.1",
    "nodemailer": "^3.1.4",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-onclickout": "^2.0.5",
    "request": "^2.79.0",
    "sharp": "^0.17.1",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babelify": "^7.3.0",
    "browserify": "^14.1.0",
    "brunch": "^2.10.3",
    "cssnano": "^3.10.0",
    "eslint": "^3.10.2",
    "eslint-plugin-react": "^6.7.1",
    "node-sass": "^4.5.0",
    "nodemon": "^1.11.0",
    "postcss-brunch": "^2.0.5",
    "postcss-cli": "^3.0.0-beta",
    "react-router": "^3.0.0",
    "sass-brunch": "^2.9.0",
    "uglify-js": "^2.8.3"
  }
}
