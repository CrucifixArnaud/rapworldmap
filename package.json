{
  "name": "rapworldmap",
  "version": "1.0.0",
  "description": "Display a map of Rapper based on their childhood neighborhood",
  "main": "rapworldmap.js",
  "scripts": {
    "test": "mocha",
    "dev:test": "mocha -w",
    "dev:css": "nodemon -e app/assets/scss/main.scss -x \"npm run compile:css && npm run autoprefixer\"",
    "dev:js": "watchify -t [ babelify ] app/assets/js/atlas.js -o public/js/atlas.bundle.js",
    "compile:css": "node-sass --include-path scss app/assets/scss/main.scss public/css/main.css",
    "autoprefixer": "postcss -um autoprefixer -r public/css/main.css",
    "minify:css": "postcss -u cssnano -r public/css/main.css",
    "browserify": "NODE_ENV=production browserify app/assets/js/atlas.js -t [ babelify --presets [ es2015 react ] --plugins [ transform-class-properties transform-object-rest-spread ] ] -o public/js/atlas.bundle.js",
    "uglify:js": "NODE_ENV=production uglifyjs public/js/atlas.bundle.js -mc -o public/js/atlas.bundle.js",
    "build:css": "npm run compile:css && npm run autoprefixer && npm run minify:css",
    "build:js": "npm run browserify && npm run uglify:js",
    "build": "NODE_ENV=production npm run build:js && npm run build:css",
    "dev": "nodemon rapworldmap.js",
    "start": "NODE_ENV=production node rapworldmap.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/CrucifixArnaud/rapmap.git"
  },
  "author": "Crucifix Arnaud",
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/CrucifixArnaud/rapmap/issues"
  },
  "homepage": "https://gitlab.com/CrucifixArnaud/rapmap#README",
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "1.15.2",
    "client-sessions": "0.7.0",
    "connect-flash": "0.1.1",
    "cookie-parser": "1.4.3",
    "dotenv": "2.0.0",
    "ejs": "2.5.2",
    "express": "4.14.0",
    "express-ejs-layouts": "2.2.0",
    "express-validator": "2.21.0",
    "leaflet.markercluster": "1.0.0",
    "mapbox.js": "3.0.1",
    "moment": "2.15.2",
    "mongoose": "4.6.4",
    "morgan": "1.7.0",
    "multer": "1.2.1",
    "nodemailer": "3.1.4",
    "passport": "0.3.2",
    "passport-local": "1.0.0",
    "react": "15.3.2",
    "react-dom": "15.3.2",
    "react-motion": "0.4.7",
    "react-onclickout": "2.0.5",
    "request": "2.79.0",
    "sharp": "0.17.1",
    "underscore": "1.8.3"
  },
  "devDependencies": {
    "babel-cli": "6.18.0",
    "babel-plugin-transform-class-properties": "6.23.0",
    "babel-plugin-transform-object-rest-spread": "6.23.0",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-react": "6.23.0",
    "babelify": "7.3.0",
    "browserify": "14.1.0",
    "cssnano": "3.10.0",
    "eslint": "3.10.2",
    "eslint-plugin-react": "6.7.1",
    "mocha": "^3.2.0",
    "node-sass": "4.5.0",
    "nodemon": "1.11.0",
    "postcss-cli": "3.0.0-beta",
    "react-router": "3.0.0",
    "should": "^11.2.1",
    "uglify-js": "2.8.3"
  }
}
